{
  "name": "durancr-io",
  "version": "0.1.0",
  "private": false,
  "scripts": {
    "build": "next build",
    "dev": "next",
    "export": "next export",
    "lint": "eslint src",
    "lint:fix": "npm run lint -- --fix",
    "preexport": "npm run build",
    "prestart": "npm run export",
    "start": "serve out",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "lint-staged": {
    "**/*.{js,jsx}": [
      "npm run --silent lint:fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test"
    }
  },
  "dependencies": {
    "@zeit/next-sass": "^1.0.1",
    "classnames": "^2.2.6",
    "contentful": "^7.13.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "lodash.get": "^4.4.2",
    "next": "9.3.2",
    "node-sass": "^4.12.0",
    "prop-types": "^15.7.2",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.6.3",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "enzyme-to-json": "^3.4.2",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "husky": "^3.0.9",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "lint-staged": "^9.4.2",
    "serve": "^11.3.0"
  }
}
